<FindBugsFilter>
  <Match>
    <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,THROWS_METHOD_THROWS_CLAUSE_BASIC_EXCEPTION,THROWS_METHOD_THROWS_RUNTIMEEXCEPTION"/>
  </Match>
  <Match>
    <Class name="org.jboss.pnc.build.finder.cli.Main"/>
    <Bug pattern="DM_EXIT"/>
  </Match>
  <Match>
    <Class name="org.jboss.pnc.build.finder.core.DistributionAnalyzer"/>
    <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE,RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE"/>
  </Match>
  <Match>
    <Class name="org.jboss.pnc.build.finder.core.Utils"/>
    <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE"/>
  </Match>
  <Match>
    <Class name="org.jboss.pnc.build.finder.koji.KojiClientSession"/>
    <Bug pattern="MC_OVERRIDABLE_METHOD_CALL_IN_CONSTRUCTOR"/>
  </Match>
  <!-- code auto-generated by protostream are flagged by spotbugs -->
  <Match>
    <Package name="org.jboss.pnc.build.finder.protobuf"/>
    <Bug pattern="DLS_DEAD_LOCAL_STORE,RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE"/>
  </Match>
</FindBugsFilter>
